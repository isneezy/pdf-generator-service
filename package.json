{
  "name": "pdf-generator",
  "version": "0.4.0",
  "description": "A simple express service that generates a pdf based on the submitted HTML",
  "homepage": "https://github.com/isneezy/pdf-generator-service",
  "repository": {
    "type": "git",
    "url": "https://github.com/isneezy/pdf-generator-service.git"
  },
  "bugs": "https://github.com/isneezy/pdf-generator-service/issues",
  "author": "Ivan Vilanculo <vilanculoivan@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "lerna run build",
    "pretest": "lerna run build",
    "test": "vitest",
    "lint": "eslint --ext .js,.ts --ignore-path .gitignore .",
    "coverage": "yarn test --coverage"
  },
  "devDependencies": {
    "@qiwi/multi-semantic-release": "^6.5.1",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/git": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "@vitest/coverage-c8": "^0.27.3",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "lerna": "^6.1.0",
    "prettier": "^2.8.3",
    "typescript": "^4.9.4",
    "vitest": "^0.27.3"
  },
  "workspaces": [
    "packages/*"
  ],
  "multi-release": {
    "dryRun": true,
    "debug": false,
    "tagFormat": "${name}@${version}"
  }
}
